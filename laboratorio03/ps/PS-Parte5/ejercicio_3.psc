Algoritmo es_mayo 
Escribir "Digite el primer número: "
Leer num1

Escribir "Digite el segundo número: "
Leer num2

Si num1 == num2 Entonces
	
	Escribir "Los dos números son iguales"
	
SiNo
	
	Si num1 > num2 Entonces
		
		Escribir "El número mayor es: ",num1
		
		
		
	SiNo
		
		Escribir "El número mayor es: ",num2
		
		
		
	Fin Si
	
Fin Si

FinAlgoritmo
	
